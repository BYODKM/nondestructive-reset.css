include /helper/nondestructive-reset
- var normalizeCss = "https://cdnjs.cloudflare.com/ajax/libs/normalize/3.0.2/normalize.css"

doctype
+html
  head
    meta(charset="utf-8")
    title Non-destructive Reset CSS Test
    link(rel="stylesheet" href="#{normalizeCss}")
    link(rel="stylesheet" href="../nondestructive-reset.css")
  +body

    h1 h1
    h2 h2
    h3 h3
    h4 h4
    h5 h5
    h6 h6
    +h1 h1.h1
    +h2 h2.h2
    +h3 h3.h3
    +h4 h4.h4
    +h5 h5.h5
    +h6 h6.h6

    +hr

    p p
    +p p.p

    +hr

    div
      a(href="") a
    div
      +a(href="") a.a

    +hr

    div
      cite cite
    div
      +cite cite.cite

    +hr

    div
      code code
    div
      +code code.code

    +hr

    ol
      li ol
      li li
    +ol
      +li ol.ol
      +li li.li

    +hr

    ul
      li ul
      li li
    +ul
      +li ul.ul
      +li li.li

    +hr

    dl
      dt dt
      dd dd

    +dl
      +dt dt.dt
      +dd dd.dd

    +hr

    blockquote blockquote
    +blockquote blockquote.blockquote

    +hr

    pre pre
    pre
      code code
    +pre pre.pre
    +pre
      +code code.code

    +hr

    table
      tr
        th
          | th
          br
          | th
        td td
      tr
        th th
        td
          | td
          br
          | td
    +table
      +tr
        +th
          | th.th
          br
          | th.th
        +td td.td
      +tr
        +th th.th
        +td
          | td.td
          br
          | td.td

    +hr

    div
      input(value="input")
      input(value="input")
    div
      +input(value="input.input")
      +input(value="input.input")

    +hr

    textarea textarea
    +textarea textarea.textarea

    +hr

    div
      select
        option select
        option select
        option select
      select
        option select
        option select
        option select
    div
      +select
        option select.select
        option select.select
        option select.select
      +select
        option select.select
        option select.select
        option select.select

    +hr

    div
      button button
      button button
    div
      +button button.button
      +button button.button

    +hr

    div
      label label
    div
      +label label.label

    +hr

    address address
    +address address.address

    +hr
    
    fieldset
      legend fieldset

    +fieldset
      legend fieldset.fieldset

    +hr

    iframe(src="")
    +iframe(src="")

    script(src="//localhost:35729/livereload.js")
